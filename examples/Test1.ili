INTERLIS 2.4;

MODEL TestModel (de) AT "http://models.geow.cloud" VERSION "1" =

IMPORTS Time;
    TOPIC TestTopic =

        CLASS ClassA =
            attrA: TEXT*10;
            attrTime: Time.DayOfYear;
        END ClassA;

        CLASS ClassB =
            attrB: 10..20;
        END ClassB;

        ASSOCIATION Assoc1 =
            AssocA -- {0..*} ClassA;
            AssocB -<> {1} ClassB;
        END Assoc1;
    END TestTopic;
END TestModel.
